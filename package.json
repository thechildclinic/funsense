{
  "name": "school-health-screening-netlify",
  "version": "1.0.0",
  "description": "School Health Screening System with Netlify Functions",
  "private": true,
  "type": "module",
  "dependencies": {
    "@google/genai": "^1.6.0",
    "@netlify/functions": "^4.1.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "recharts": "^2.15.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
